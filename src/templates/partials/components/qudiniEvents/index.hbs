<div class="qudiniEvents section">

  <div class="qudiniEvents__header">
    <div class="qudiniEvents__date qudiniEvents__filter-group">
      <input placeholder="Start Date" type="date" id="filter-date-start"/>
      <input placeholder="End Date" type="date" id="filter-date-end"/>
    </div>

    <div class="qudiniEvents__category qudiniEvents__filter-group">
      <input type="checkbox" id="filter-category-entertainment"/><label for="filter-category-entertainment">{{>toggle color="#0077C8"}}Entertainment</label>
      <input type="checkbox" id="filter-category-innovation"/><label for="filter-category-innovation">{{>toggle color="#00C3B2"}}Tech & Innovation</label>
      <input type="checkbox" id="filter-category-creativity"/><label for="filter-category-creativity">{{>toggle color="#FF4337"}}Creativity</label>
      <input type="checkbox" id="filter-category-lifestyle"/><label for="filter-category-lifestyle">{{>toggle color="#FFB546"}}Lifestyle & Wellbeing</label>
    </div>

    <div class="qudiniEvents__type qudiniEvents__filter-group">
      <input type="radio" id="filter-type-virtual" name="filter-type" value="virtual">
      <label for="filter-type-virtual">{{>virtual_icon}}Virtual events</label>
      <input type="radio" id="filter-type-live" name="filter-type" value="live">
      <label for="filter-type-live">{{>live_icon}}Live events</label>
      <input type="radio" id="filter-type-alwayson" name="filter-type" value="alwayson">
      <label for="filter-type-alwayson">{{>alwayson_icon}}Always on</label>
    </div>
  </div>

  <div class="qudiniEvents__body" id="qudini-events-body">
    <div class="qudiniEvents__loader" id="qudini-events-loader"></div>
    <div class="qudiniEvents__events" id="qudini-events-target"></div>
  </div>

  <div class="qudiniEvents__more">
    <a class="btn btn--primary btn--whatson" id="qudini-events-more">Show More</a>
  </div>

</div>

{{#*inline "toggle"}}
<!-- <div class="toggle_switch"> -->
  <div class="qudiniSwitch">
    <div class="qudiniSwitch__before"></div>
    <div class="qudiniSwitch__after" style="background-color:{{color}};"></div>
  </div>
<!-- </div> -->
{{/inline}}

