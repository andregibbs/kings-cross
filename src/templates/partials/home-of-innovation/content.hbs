
<div class="hoiContent">

  {{#each components}}
  	{{#if_equals this.type 'kv'}}
  		{{> home-of-innovation/contentKv this }}
  	{{/if_equals}}
  	{{#if_equals this.type 'headline'}}
  		{{> home-of-innovation/contentHeadline this }}
  	{{/if_equals}}
  	{{#if_equals this.type 'intro'}}
  		{{> home-of-innovation/contentIntro this }}
  	{{/if_equals}}
  	{{#if_equals this.type 'paras'}}
  		{{> home-of-innovation/contentParas this }}
  	{{/if_equals}}
  	{{#if_equals this.type 'gallery'}}
  		{{> home-of-innovation/contentGallery this }}
  	{{/if_equals}}
    {{#if_equals this.type 'spacer'}}
  		{{> home-of-innovation/contentSpacer this }}
  	{{/if_equals}}
    {{#if_equals this.type 'group'}}
    	{{> home-of-innovation/contentGroup this }}
    {{/if_equals}}
    {{#if_equals this.type 'related' }}
    	{{> home-of-innovation/contentRelated this }}
    {{/if_equals}}
    {{#if_equals this.type 'items' }}
    	{{> home-of-innovation/contentItems this }}
    {{/if_equals}}
  {{/each}}

</div>
